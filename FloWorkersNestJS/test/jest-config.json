{
  "testRunner": "jest-jasmine2",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../workers",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest",
    "^.+\\.(?!js)(?!jsx)$": "ts-jest"
  },
  "coverageDirectory": "../report/coverage",
  "testResultsProcessor": "jest-sonar-reporter",
  "setupFilesAfterEnv": [
    "jest-allure/dist/setup",
    "<rootDir>../test/allure-config.js"
  ]
}