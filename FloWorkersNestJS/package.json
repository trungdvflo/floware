{
  "name": "worker-4.2",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "fix-lint": "tslint --fix -c ./tslint.json 'src/**/*.ts'",
    "test": "rimraf report && jest --config test/jest-config.json --detectOpenHandles --coverage --forceExit",
    "test:single": "jest --config test/jest-config.json --detectOpenHandles -i",
    "report:generate": "allure generate report/allure-results --clean -o report/allure-report",
    "report:serve": "allure serve report/allure-results",
    "worker-api-last-modify": "nest build && node build/worker-api-last-modify/main",
    "worker-links-object": "nest build && node build/worker-links-object/main",
    "worker-manual-rules": "nest build && node build/worker-manual-rules/main",
    "worker-collection": "nest build && node build/worker-collection/main",
    "worker-kanban": "nest build && node build/worker-kanban/main",
    "worker-common": "nest build && node build/worker-common/main",
    "worker-object": "nest build && node build/worker-object/main",
    "worker:getList": "nest build && node build/list-worker",
    "worker-third-party-account": "nest build && node build/worker-third-party-account/main",
    "worker-trash-collection": "nest build && node build/worker-trash-collection/main",
    "worker-delete-caldav": "nest build && node build/worker-delete-caldav/main",
    "worker-delete-child-collection": "nest build && node build/worker-delete-child-collection/main",
    "cronjob:invalid-data": "nest build && node build/worker-invalid-cronjob/main",
    "worker:invalid-data-deletion": "nest build && node build/worker-invalid-data-deletion/main",
    "worker:invalid-flomail-collector": "nest build && node build/worker-invalid-flomail-collector/main",
    "worker:invalid-flo-object-collector": "nest build && node build/worker-invalid-flo-object-collector/main",
    "cronjob:collection-notification": "nest build && node build/worker-notification-cronjob/main",
    "worker:outdated-notification-cleaner": "nest build && node build/worker-outdated-notification-deletion/main",
    "worker:realtime": "nest build && node build/worker-realtime/main"
  },
  "jestSonar": {
    "reportPath": "report/coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "@anchan828/typeorm-transformers": "^1.0.32",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "0.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "9.3.10",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^9.4.2",
    "@nestjs/platform-express": "9.0.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@parse/node-apn": "5.2.1",
    "@types/bull": "^3.15.9",
    "@types/nodemailer": "^6.4.14",
    "amqplib": "^0.10.3",
    "aws-sdk": "2.1354.0",
    "bull": "^4.12.2",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "cron": "2.3.1",
    "decimal.js": "^10.4.2",
    "digest-fetch": "^2.0.1",
    "dotenv": "16.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "graylog2": "^0.2.1",
    "i": "^0.3.7",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.12",
    "npm": "^9.8.0",
    "re2": "^1.20.10",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.3.11",
    "@types/cache-manager": "^4.0.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "^28.1.2",
    "jest-allure": "^0.1.3",
    "jest-jasmine2": "^28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "tslint": "^6.1.3",
    "typescript": "4.7.4"
  },
  "overrides": {
    "@parse/node-apn": {
      "jsonwebtoken": "9.0.0"
    }
  }
}
