{
  "name": "floapinodejs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "export RABBIT_MQ_ENABLE=true && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:upload": "nest build && node dist/apps/upload-download/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "fix-lint": "tslint --fix -c ./tslint.json 'src/**/*.ts'",
    "test": "rimraf report && jest --config test/jest-config.json --detectOpenHandles --coverage",
    "test:single": "jest --config test/jest-config.json --detectOpenHandles -i",
    "report:generate": "allure generate report/allure-results --clean -o report/allure-report",
    "report:serve": "allure serve report/allure-results",
    "generate-routes": "node --nolazy -r ts-node/register -r tsconfig-paths/register scripts/generate-routes.ts",
    "generate-routes-upload": "node --nolazy -r ts-node/register -r tsconfig-paths/register scripts/upload-download.generate-routes.ts"
  },
  "jestSonar": {
    "reportPath": "report/coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "@anchan828/typeorm-transformers": "^0.4.25",
    "@nestjs/axios": "3.0.2",
    "@nestjs/bull": "0.6.0",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "9.3.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "9.0.1",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@parse/node-apn": "^5.2.3",
    "@slack/web-api": "^7.0.2",
    "@types/multer": "^1.4.5",
    "@types/uuid": "^8.3.0",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.879.0",
    "axios": "^1.6.4",
    "bull": "^4.12.0",
    "bullmq": "^4.17.0",
    "cache-manager": "^3.6.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "compare-versions": "^3.6.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "graylog2": "^0.2.1",
    "ioredis": "^5.2.4",
    "jimp": "^0.22.12",
    "mysql": "^2.18.1",
    "re2": "1.20.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.3.1",
    "@types/bull": "^3.15.8",
    "@types/cache-manager": "^3.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.2",
    "jest": "^27.2.5",
    "jest-allure": "^0.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "overrides": {
    "@nestjs/axios": {
      "follow-redirects": "^1.15.6"
    }
  }
}
